<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Lab</title>
</head>
<body>
    <h1>All Students</h1>
    <ul>
        <% students.forEach(function(student) { %>
            <li>
                <%= student.stud %>
                -
                <%= student.grad ? 'graduated' : 'not graduated' %>
            </li>
        <% }); %>


    </ul>
</body>
</html>